{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "TZ=UTC vitest run",
    "test:unit:watch": "TZ=UTC vitest watch",
    "test:a11y": "playwright test --project=a11y",
    "test:e2e": "playwright test",
    "test:e2e:caselaw": "playwright test test/e2e/caselaw",
    "test:e2e:norms": "playwright test test/e2e/norms",
    "test:e2e:shared": "playwright test test/e2e/shared",
    "coverage": "TZ=UTC vitest run --coverage",
    "check": "npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "dev": "vite",
    "prettier:check": "f() { prettier --check ${@:-.}; }; f",
    "prettier:fix": "f() { prettier --write ${@:-.}; }; f",
    "eslint:check": "f() { eslint --max-warnings=0 ${@:-.}; }; f",
    "eslint:fix": "f() { eslint --fix --max-warnings=0 ${@:-.}; }; f",
    "stylelint:check": "f() { stylelint ${@:-.}; }; f",
    "stylelint:fix": "f() { stylelint --fix ${@:-.}; }; f",
    "style:check": "f() { vue-tsc && npm run prettier:check $@ && npm run eslint:check $@ && npm run stylelint:check $@; }; f",
    "style:fix": "f() { npm run prettier:fix $@ && npm run eslint:fix $@ && npm run stylelint:fix $@; }; f",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.4",
    "@sentry/vue": "^7.73.0",
    "@tiptap/extension-bold": "^2.1.9",
    "@tiptap/extension-bullet-list": "^2.1.9",
    "@tiptap/extension-color": "^2.1.9",
    "@tiptap/extension-document": "^2.1.9",
    "@tiptap/extension-history": "^2.1.9",
    "@tiptap/extension-image": "^2.1.9",
    "@tiptap/extension-italic": "^2.1.9",
    "@tiptap/extension-list-item": "^2.1.9",
    "@tiptap/extension-mention": "^2.1.9",
    "@tiptap/extension-ordered-list": "^2.1.9",
    "@tiptap/extension-paragraph": "^2.1.9",
    "@tiptap/extension-strike": "^2.1.9",
    "@tiptap/extension-subscript": "^2.1.9",
    "@tiptap/extension-superscript": "^2.1.9",
    "@tiptap/extension-table": "^2.1.9",
    "@tiptap/extension-table-cell": "^2.1.9",
    "@tiptap/extension-table-header": "^2.1.9",
    "@tiptap/extension-table-row": "^2.1.9",
    "@tiptap/extension-text": "^2.1.9",
    "@tiptap/extension-text-align": "^2.1.9",
    "@tiptap/extension-text-style": "^2.1.9",
    "@tiptap/extension-underline": "^2.1.9",
    "@tiptap/suggestion": "^2.1.9",
    "@tiptap/vue-3": "^2.1.9",
    "@unleash/proxy-client-vue": "^0.0.8",
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "immer": "^10.0.2",
    "maska": "^2.1.10",
    "material-icons": "^1.13.11",
    "pinia": "^2.1.6",
    "resize-observer-polyfill": "^1.5.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.3",
    "vue-router": "4.2.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.3",
    "@digitalservice4germany/angie": "^1.1.0",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@namics/stylelint-bem": "^9.0.0",
    "@playwright/test": "^1.38.0",
    "@sentry/vite-plugin": "^2.8.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
