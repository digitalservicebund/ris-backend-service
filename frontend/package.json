{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=24.11.0"
  },
  "type": "module",
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:e2e": "playwright test test/e2e",
    "test:a11y": "playwright test --project=a11y",
    "test:queries": "playwright test --project=queries --reporter=./test/queries/queries_log_reporter.ts --workers 1",
    "coverage": "vitest run --coverage",
    "check": "npm run style:fix && npm test && npm run test:e2e -- --project='chromium' --retries=1 && npm run test:a11y",
    "dev": "vite --host",
    "type:check": "f() { vue-tsc --incremental --noEmit; }; f",
    "prettier:check": "f() { prettier --check  --cache --cache-location .cache/prettier/ ${@:-.}; }; f",
    "prettier:fix": "f() { prettier --write --cache --cache-location .cache/prettier/ ${@:-.}; }; f",
    "eslint:check": "f() { eslint --no-warn-ignored --max-warnings=0  --cache --cache-strategy content --cache-location .cache/eslint/ ${@:-.}; }; f",
    "eslint:fix": "f() { eslint --fix --no-warn-ignored --max-warnings=0  --cache --cache-strategy content --cache-location .cache/eslint/ ${@:-.}; }; f",
    "stylelint:check": "f() { stylelint  --cache --cache-location .cache/stylelint/ ${@:-.}; }; f",
    "stylelint:fix": "f() { stylelint  --cache --cache-location .cache/stylelint/ ${@:-.} --fix; }; f",
    "style:check": "f() { npm run type:check && npm run prettier:check $@ && npm run eslint:check $@ && npm run stylelint:check $@; }; f",
    "style:fix": "f() { npm run type:check && npm run prettier:fix $@ && npm run eslint:fix $@ && npm run stylelint:fix $@; }; f",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@digitalservicebund/ris-ui": "^3.15.1",
    "@guardian/prosemirror-invisibles": "^3.1.1",
    "@sentry/integrations": "^7.114.0",
    "@sentry/vue": "^10.22.0",
