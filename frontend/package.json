{
  "name": "neuris-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preserve": "vite build",
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "clean": "rm -rf dist/",
    "test": "TZ=UTC vitest run",
    "test:unit:watch": "TZ=UTC vitest watch",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:caselaw": "playwright test test/e2e/caselaw",
    "test:e2e:norms": "playwright test test/e2e/norms",
    "test:e2e:shared": "playwright test test/e2e/shared",
    "test:a11y": "playwright test --project=a11y",
    "test:queries": "playwright test --project=queries --reporter=./test/queries/queries_log_reporter.ts",
    "coverage": "TZ=UTC vitest run --coverage",
    "check": "npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "dev": "vite",
    "prettier:check": "f() { prettier --check ${@:-.}; }; f",
    "prettier:fix": "f() { prettier --write ${@:-.}; }; f",
    "eslint:check": "f() { eslint --max-warnings=0 ${@:-.}; }; f",
    "eslint:fix": "f() { eslint --fix --max-warnings=0 ${@:-.}; }; f",
    "stylelint:check": "f() { stylelint ${@:-.}; }; f",
    "stylelint:fix": "f() { stylelint --fix ${@:-.}; }; f",
    "style:check": "f() { vue-tsc && npm run prettier:check $@ && npm run eslint:check $@ && npm run stylelint:check $@; }; f",
    "style:fix": "f() { npm run prettier:fix $@ && npm run eslint:fix $@ && npm run stylelint:fix $@; }; f",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow \"$(. ./parseAllowedLicences.sh)\" --csv --out ./frontend-licence-report.csv"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.0.0",
    "@sentry/integrations": "^7.107.0",
    "@sentry/vue": "^7.107.0",
    "@tiptap/extension-bold": "^2.2.4",
    "@tiptap/extension-bullet-list": "^2.2.4",
    "@tiptap/extension-color": "^2.2.4",
    "@tiptap/extension-document": "^2.2.4",
    "@tiptap/extension-history": "^2.2.4",
    "@tiptap/extension-image": "^2.2.4",
    "@tiptap/extension-italic": "^2.2.4",
    "@tiptap/extension-list-item": "^2.2.4",
    "@tiptap/extension-ordered-list": "^2.2.4",
    "@tiptap/extension-paragraph": "^2.2.4",
    "@tiptap/extension-strike": "^2.2.4",
    "@tiptap/extension-subscript": "^2.2.4",
    "@tiptap/extension-superscript": "^2.2.4",
    "@tiptap/extension-table": "^2.2.4",
    "@tiptap/extension-table-cell": "^2.2.4",
    "@tiptap/extension-table-header": "^2.2.4",
    "@tiptap/extension-table-row": "^2.2.4",
    "@tiptap/extension-text": "^2.2.4",
    "@tiptap/extension-text-align": "^2.2.4",
    "@tiptap/extension-text-style": "^2.2.4",
    "@tiptap/extension-underline": "^2.2.4",
    "@tiptap/vue-3": "^2.2.4",
    "@unleash/proxy-client-vue": "^0.1.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "immer": "^10.0.4",
    "maska": "^2.1.11",
    "resize-observer-polyfill": "^1.5.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.4",
    "vue-router": "4.3.0",
    "vue-tsc": "^2.0.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@digitalservice4germany/angie": "^1.1.1",
    "@digitalservice4germany/style-dictionary": "^2.0.0",
    "@iconify-json/ic": "^1.1.17",
    "@iconify-json/material-symbols": "^1.1.75",
    "@namics/stylelint-bem": "^10.0.0",
    "@playwright/test": "^1.42.1",
    "@sentry/vite-plugin": "^2.16.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.18",
    "axe-playwright": "^2.0.1",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-vue-scoped-css": "^2.7.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "jest-fail-on-console": "^3.1.2",
    "jsdom": "^24.0.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.42.1",
    "postcss": "^8.4.36",
    "postcss-import": "^16.1.0",
    "postcss-scrollbar": "^0.5.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.2.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pages": "^0.32.0",
    "vitest": "^1.4.0"
  }
}
