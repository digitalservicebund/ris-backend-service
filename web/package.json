{
  "name": "typescript-vite-application-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "test:e2e": "playwright test",
    "audit:licences": "license-checker --production --excludePrivatePackages --onlyAllow ''",
    "check": "vue-tsc --noEmit && npm run style:check && npm test && npm run test:e2e && npm run test:a11y",
    "clean": "rm -rf dist/",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint:check -- --fix",
    "preserve": "vite build",
    "style:check": "npm run format:check && npm run lint:check",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "test": "vitest run",
    "test:a11y": "playwright test --config=test/a11y/playwright.config.ts"
  },
  "dependencies": {
    "@pinia/testing": "^0.0.12",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/vue-3": "^2.0.0-beta.91",
    "dayjs": "^1.11.3",
    "material-design-icons-iconfont": "^6.7.0",
    "pinia": "^2.0.14",
    "roboto-fontface": "*",
    "upath": "^2.0.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@digitalservice4germany/style-dictionary": "^0.0.4",
    "@playwright-testing-library/test": "^4.2.1",
    "@playwright/test": "^1.22.2",
    "@testing-library/dom": "^8.13.0",
    "@types/node": "^17.0.42",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.0",
    "autoprefixer": "^10.4.7",
    "axe-playwright": "^1.1.11",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "jsdom": "^19.0.0",
    "license-checker": "^25.0.1",
    "playwright": "^1.22.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.14.2",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.37.8",
    "vuetify-loader": "^2.0.0-alpha.0"
  }
}
